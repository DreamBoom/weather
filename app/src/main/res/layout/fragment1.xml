<?xml version="1.0" encoding="utf-8"?>
<yfkj.zm.farm.view.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/f6"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<RelativeLayout
    android:id="@+id/r1"
    app:width_percent="0.2"
    android:layout_margin="10dp"
    android:background="@drawable/bg_bg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:ignore="MissingPrefix">
    <TextView
        android:id="@+id/t1"
        android:paddingTop="10dp"
        android:paddingStart="10dp"
        android:lines="1"
        android:textSize="@dimen/size15"
        android:textColor="@color/black3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/stationId"
        android:layout_below="@+id/t1"
        android:paddingTop="10dp"
        android:paddingStart="10dp"
        android:paddingBottom="7dp"
        android:text="点击登录"
        android:textSize="@dimen/size15"
        android:textColor="@color/black3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/bbh"
        android:layout_below="@+id/stationId"
        android:paddingStart="10dp"
        android:paddingBottom="10dp"
        android:text="系统版本:"
        android:textSize="@dimen/size15"
        android:textColor="@color/black3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</RelativeLayout>
<include
    android:layout_below="@+id/r1"
    app:width_percent="0.2"
    android:layout_marginStart="10dp"
    android:layout_marginBottom="10dp"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    layout="@layout/f1_r2"
    tools:ignore="MissingPrefix" />
    <include
        android:id="@+id/r3"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        app:height_percent="0.5"
        android:layout_toEndOf="@+id/r1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/f1_r3"
        tools:ignore="MissingPrefix" />
    <include
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="10dp"
        android:layout_below="@+id/r3"
        app:height_percent="0.47"
        android:layout_toEndOf="@+id/r1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/f1_r4"
        tools:ignore="MissingPrefix" />
</yfkj.zm.farm.view.PercentRelativeLayout>
